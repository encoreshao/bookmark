{
  "manifest_version": 2,
  "name": "Analyze Location",
  "short_name": "Bookmark",
  "description": "Bookmark Extension",
  "version": "0.9",
  "background": {
    "scripts": ["js/event.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "tabs", 
    "http://*/*", 
    "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["src/content/content.js"]
  }],
  "update_url": "https://clients2.google.com/service/update2/crx"
}